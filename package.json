{
    "name": "red_thread",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "db:generate": "wireit",
        "db:migrate_prod": "wireit",
        "db:migrate_dev": "wireit",
        "db:migrate": "wireit",
        "db:studio": "./node_modules/@dotenvx/dotenvx/src/cli/dotenvx.js run -f ./.env.local -- npx prisma studio",
        "db:studio_prod": "./node_modules/@dotenvx/dotenvx/src/cli/dotenvx.js run -f ./.env.prod -- npx prisma studio",
        "dev": "next dev",
        "build": "wireit",
        "start": "next start",
        "typecheck": "tsc --noEmit",
        "lint": "eslint",
        "test": "pnpm dlx vitest",
        "test:ui": "pnpm dlx vitest --ui",
        "test:run": "pnpm dlx vitest run"
    },
    "wireit": {
        "db:migrate_prod": {
            "command": "./node_modules/@dotenvx/dotenvx/src/cli/dotenvx.js run -f ./.env.prod -- npx prisma migrate deploy"
        },
        "db:migrate": {
            "command": "./node_modules/@dotenvx/dotenvx/src/cli/dotenvx.js run -f ./.env.local -- npx prisma migrate dev"
        },
        "db:migrate_dev": {
            "command": "./node_modules/@dotenvx/dotenvx/src/cli/dotenvx.js run -f ./.env.local -- npx prisma migrate deploy"
        },
        "db:generate": {
            "command": "npx prisma generate"
        },
        "build:velite": {
            "command": "pnpm velite build"
        },
        "build": {
            "dependencies": [
                "build:velite",
                "db:generate"
            ],
            "command": "next build"
        }
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.975.0",
